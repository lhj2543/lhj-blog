package com.uniwcp.pojofactory.jdbc;

import java.util.ArrayList;
import java.util.HashSet;
import java.util.List;
import java.util.Set;

public class TableMeta {
	
	private String name;
	private String nameCn;
	private String className;
	private String packageName;
	private Set<String> dependens=new HashSet<String>();
	
	private List<FieldMeta> fields=new ArrayList<FieldMeta>();
	
	
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getNameCn() {
		return nameCn;
	}
	public void setNameCn(String nameCn) {
		this.nameCn = nameCn;
	}
	public String getClassName() {
		return className;
	}
	public void setClassName(String className) {
		this.className = className;
	}
	public String getPackageName() {
		return packageName;
	}
	public void setPackageName(String packageName) {
		this.packageName = packageName;
	}
	public List<FieldMeta> getFields() {
		return fields;
	}
	public void setFields(List<FieldMeta> fields) {
		this.fields = fields;
	}
	/**
	 * @return the dependens
	 */
	public Set<String> getDependens() {
		return dependens;
	}
	/**
	 * @param dependens the dependens to set
	 */
	public void setDependens(Set<String> dependens) {
		this.dependens = dependens;
	}

}
